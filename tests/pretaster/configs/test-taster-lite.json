{
  "name": "pretaster-taster-test",
  "version": "1.0.0",
  "description": "Pretaster test using real taster",
  "command": "/bin/bash {workenv}/scripts/test-with-taster.sh",
  "environment": {
    "TASTER_MODE": "real"
  },
  "package": {
    "name": "pretaster-taster-test",
    "version": "1.0.0",
    "description": "Pretaster test using real taster"
  },
  "format": "PSPF/2025",
  "slots": [
    {
      "slot": 0,
      "purpose": "payload",
      "lifecycle": "cached",
      "operations": "raw",
      "id": "test-with-taster-sh",
      "source": "{workenv}/scripts/test-with-taster.sh",
      "target": "scripts/test-with-taster.sh",
      "permissions": "0755"
    },
    {
      "slot": 1,
      "purpose": "payload",
      "lifecycle": "cached",
      "operations": "raw",
      "id": "taster-psp",
      "source": "{workenv}/scripts/taster.psp",
      "target": "bin/taster.psp"
    }
  ],
  "runtime": {
    "env": {
      "unset": [
        "*"
      ],
      "pass": [
        "PATH",
        "HOME",
        "USER",
        "LANG",
        "LC_*",
        "TERM",
        "SHELL",
        "FLAVOR_*",
        "TASTER_PSP"
      ],
      "set": {
        "PRETASTER_VERSION": "1.0.0"
      }
    }
  },
  "execution": {
    "command": "/bin/bash {workenv}/scripts/test-with-taster.sh",
    "args": [],
    "env": {
      "TASTER_MODE": "real"
    },
    "primary_slot": 0
  }
}