{
  "name": "pretaster-env",
  "version": "1.0.0",
  "description": "Environment variable filtering test",
  "package": {
    "name": "pretaster-env",
    "version": "1.0.0",
    "description": "Environment variable filtering test"
  },
  "format": "PSPF/2025",
  "slots": [
    {
      "slot": 0,
      "id": "env-test-script",
      "source": "{workenv}/scripts/env_test.py",
      "target": "scripts/env_test.py",
      "purpose": "payload",
      "lifecycle": "cached",
      "operations": "none",
      "permissions": "0755"
    }
  ],
  "runtime": {
    "env": {
      "unset": ["*"],
      "pass": ["PATH", "HOME", "USER", "LANG", "LC_*", "TERM", "FLAVOR_*"],
      "set": {
        "PRETASTER_MODE": "env_test"
      }
    }
  },
  "command": "/usr/bin/python3 {workenv}/scripts/env_test.py",
  "environment": {},
  "execution": {
    "command": "/usr/bin/python3 {workenv}/scripts/env_test.py",
    "args": [],
    "env": {},
    "primary_slot": 0
  }
}